					"layer":"激活层",
					"激活函数":"ReLU函数"}
				,{
					"layer":"全连接层",
					"展开特征图":"true"}
				,{
					"layer":"归一化层",
					"shift":"1",
					"scale":"1",
					"epsilon":"-1"
				}]
		},			
		{
			"init_func":"全零",
			"W":"1,1,1,1",
			"b":"1",
			"inner_layer":[
				{
					"layer":"卷积层",
					"卷积核":"1,1,1,1",
					"步长":"1,1",
					"填充方式":"SAME"},
				{
					"layer":"池化层",
					"池化窗口大小":"1,1",
					"步长":"1,1",
					"填充方式":"SAME"}
				,{
					"layer":"激活层",
					"激活函数":"ReLU函数"}
				,{
					"layer":"全连接层",
					"展开特征图":"true"}
				,{
					"layer":"归一化层",
					"shift":"1",
					"scale":"1",
					"epsilon":"-1"
				}]
		}],
		"output_layer":{
			"init_func":"全零",
			"W":"1,1,1,1",
			"b":"1"
		}
	}
}

传统神经网络方式：
{
	"iter":"1000",
	"learning_rate":"0.1",
	"loss_name":"交叉熵函数",
	"optimizer_name":"GradientDescentOptimizer",
	"net_type":"传统神经网络",
	"net_config":{
		"hidden_layer":[{
			"init_func":"全零",
			"W":"1,1,1,1",
			"b":"1",
			"激活函数":"ReLU函数",
			"防止过拟合方法":"batch normalization"
		},
		{
			"init_func":"全零",
			"W":"1,1,1,1",
			"b":"1",
			"激活函数":"ReLU函数",
			"防止过拟合方法":"batch normalization"
		}],
		"output_layer":{
			"init_func":"全零",
			"W":"1,1,1,1",
			"b":"1",
			"激活函数":"ReLU函数",
			"防止过拟合方法":"batch normalization",
			"shift":"1",
			"scale":"1",
			"epsilon":"-1"
		}

	}
}